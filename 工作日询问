#define _CRT_SECURE_NO_WARNINGS 1
#include <stdio.h>
int main()
{
	int input = 0;
	printf("please choose a day(1-7)\n");
	scanf("%d", &input);
	if(0 < input < 6)
	{
	printf("work day\n");
}
	else if (5 < input < 8)
	{
		printf("rest day\n");
	}
	else
	{
		printf("you are a bad guy\n");
	}
	return 0;
}
