#define _CRT_SECURE_NO_WARNINGS 1
#include<stdio.h>
void main()
{
    int n, i, number;
    printf("please input n:");
    int final = 1;
    scanf("%d ", &n);
    for (i = 0; i < n; i++) 
    {
        scanf("%d", &number);
        if (number % 2 == 1) 
        {
            final *= number;
        }
    }
    printf("%d\n", final);
}
