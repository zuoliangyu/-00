#define _CRT_SECURE_NO_WARNINGS 1
#include<stdio.h>
int main()
{
    int a, b, c;
    scanf("%d%d%d", &a, &b, &c);
    if (a > b)
        if (b > c)
            printf("%d", c);
        else
            printf("%d", b);

    else if (b > c)
        if (a > c)
            printf("%d", c);
        else
            printf("%d", a);
    else
        if (a > b)
            printf("%d", b);
        else
            printf("%d", a);

     return 0;
}
